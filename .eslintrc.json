{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": "off",
        "no-multi-spaces": "off",
        "arrow-parens": "off",
        "no-multiple-empty-lines": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "object-curly-newline": "off",
        "max-len": ["error", {"code": 120, "ignoreComments": true }],
        "func-names": "off"
    }
}
